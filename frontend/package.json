    "@types/react-syntax-highlighter": "^15.5.7",
    "@types/three": "^0.158.1",
    "axios": "^1.4.0",
    "framer-motion": "^10.16.5",
    "concurrently": "^9.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-markdown": "^8.0.7",
